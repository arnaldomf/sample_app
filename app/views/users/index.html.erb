<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate %>

<ul class="users">
<!-- with no string, render uses a partial passing this variable-->
<!-- rails infers that users is a list of User, and iterates over it
  rendering the _user partial for each object -->
  <%= render @users %>
</ul>

<%= will_paginate %>
